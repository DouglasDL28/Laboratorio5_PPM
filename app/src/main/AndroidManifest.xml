<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.douglasdeleon.lab5">

    <application
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme">
        <activity android:name=".Activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".Activities.EmailActivity">
        </activity>
        <activity android:name=".Activities.ContactActivity">
        </activity>
        <activity android:name=".Activities.CreateContactActivity">
        </activity>

        <provider
                android:name=".ContactsProvider"
                android:authorities="com.example.douglasdeleon.lab5.ContactsProvider"
                android:enabled="true"
                android:exported="true">
        </provider>

        <activity android:name=".Activities.UpdateContactActivity">
        </activity>
    </application>

</manifest>